{
  "manifest_version": 2,
  "name": "UT Course Ratings",
  "description": "This extension enables ratings for UT MBA courses.",
  "version": "0.1",
  "permissions": [ "http://utmba.edu/" ],
  "permissions": [
    "storage"
   ],
  "content_scripts": [{
    "matches": ["http://utmba.edu/*"],
    "js": ["jquery.js", "app.js"],
    "run_at": "document_idle",
    "css": ["style.css"]
  }],
  "web_accessible_resources": ["inject.js"]
}
